{
    "server": {
        "port":  3000,
        "host": "localhost"
    },
    "request-limit": {
        "limit": "100kb"
    },
    "cors": {
        "origin": "*",
        "methods": ["GET", "POST", "PUT", "DELETE", "UPDATE"],
        "allowedHeaders": ["X-Requested-With", "X-HTTP-Method-Override", "Content-Type", "Accept", "Observe"]
    },
    "helmet": false,
    "enviroment": "default",
    "morganFormat": "dev",
    "rabbitmq": {
        "connectionString": "amqp://localhost:5672"
    },
    "microservice": {
        "protect": false
    }
}